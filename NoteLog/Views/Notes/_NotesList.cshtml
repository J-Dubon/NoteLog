@model IEnumerable<NoteLog.Models.Notes>

@{
}

<table id="tbNotesList" class="table dataTableClass" style="height: 100%; width: 100%;">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Titulo</th>
            <th>Tema</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var items in Model)
        {
            string btnNoteId = "btnNote" + items.Id;
            <tr>
                <td>@items.CreatedDate.ToString("dd/MM/yyyy")</td>
                <td>@items.Title</td>
                <td>@items.Subject</td>
                <td><button id="@btnNoteId" class="btn btn-sm btn-info btnNoteView">Ver &nbsp;<i class="fas fa-eye"></i></button></td>
            </tr>
        }
    </tbody>
</table>